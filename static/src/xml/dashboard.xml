<template id="template" xml:space="preserve">
	<t t-name="table_name_dashboard">
		<div class="oh_dashboard">
			<div class="container_fluid o_pos_dashboard">
            
			</div>
		</div>
	</t>
    <t t-name="main_template_dashboard">
        <div class="oh_dashboard">
            <div class="container_fluid o_pos_dashboard">
                <div class="container" style="padding-top:20px">
                    <section class="cardd-section">
                        <div class="row">
                            <div class="col-sm-12 mb-4 ">
                                <div class="row p-2">
                                    <div class="col-md-4 col-sm-5 all_products oh-data ">
                                        <div class="card ">
                                            <div class="d-flex bg-primary flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-car fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white "> Total  vehicle </h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_vehicle" t-esc="total_vehicle"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-5 all_products oh-data ">
                                        <div class="card clickable-card ">
                                            <div class="d-flex bg-success flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-user fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white "> Total Driver </h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_driver" t-esc="total_driver"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-5 all_products oh-data ">
                                        <div class="card ">
                                            <div class="d-flex bg-info flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-battery-quarter fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white ">Total Fueling Request</h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_refueling"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12 mb-4 ">
                                <div class="row p-2">
                                    <div class="col-md-4 col-sm-5 all_products oh-data pt-2 ">
                                        <div class="card ">
                                            <div class="d-flex bg-info flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-cogs fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white ">Total Services </h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_services"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-5 all_products oh-data pt-2">
                                        <div class="card ">
                                            <div class="d-flex bg-primary flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-users fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white ">Accidental Case</h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_admission"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-5 all_products oh-data pt-2">
                                        <div class="card ">
                                            <div class="d-flex bg-warning flex justify-content-between ">
                                                <div class="stat-icon p-2">
                                                    <i class="fa fa-cubes fa-7x"/>
                                                </div>
                                                <div class="px-4 mt-4 ">
                                                    <h3 class="text-white ">Total Models </h3>
                                                    <span class="text-center" id="templates">
                                                        <h2 class="text-white" id="total_model"> </h2>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-sm-12 col-md-12 d-flex justify-content-start align-item-start p-5">
                            <div class="team_ticket_div half_chart chart-container card-shadow">
                                <h2 style="margin-top:50px;">Graph</h2>
                                <hr />
                                <div class="graph_canvas">
                                    <canvas class="team_ticket_count" width="500" height="350" />
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </t>
</template>